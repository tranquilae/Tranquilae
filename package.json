  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "setup": "node scripts/setup-tranquilae.js",
    "db:migrate": "node scripts/migrate-onboarding.js",
    "db:fix-profiles": "node scripts/fix-profiles-schema.js",
    "db:test": "node scripts/test-database.js",
    "test:emails": "node -e \"require('./lib/email.ts').verifyEmailConfig()\"",
    "test:stripe": "node -e \"console.log('Testing Stripe...'); const stripe = require('stripe')(process.env.STRIPE_SECRET_KEY); stripe.accounts.retrieve().then(acc => console.log('‚úÖ Stripe connected:', acc.id)).catch(e => console.error('‚ùå Stripe error:', e.message))\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "validate-env": "node -e \"require('./lib/validation').validateEnvironment(); console.log('‚úÖ Environment validated')\"",
    "test:jwt": "node scripts/test-jwt-config.js",
    "security-audit": "npm audit --audit-level=moderate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "scrape:videos": "node scripts/scrape-workout-videos.js",
    "ingest:media": "node scripts/trigger-ingest.js",
    "postinstall": "echo 'üåø Tranquilae installed! Run npm run setup to configure your system.'"
