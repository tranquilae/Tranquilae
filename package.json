    "type-check:watch": "tsc --noEmit --watch",
    "validate-env": "node -e \"require('./lib/validation').validateEnvironment(); console.log('‚úÖ Environment validated')\"",
    "security-audit": "npm audit --audit-level=moderate",
    "test": "jest",
    "test:watch": "jest --watch",
